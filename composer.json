{
    "name": "potherca/apache-directory-list-theming",
    "description": "A nicer looking directory listing for Apache using PHP, HTML, CSS and the Apache AutoIndex Module",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Ben Peachey",
            "email": "potherca@gmail.com"
        }
    ],
    "require": {
        "fxp/composer-asset-plugin": "^1.1",
        "bower-asset/bootstrap": "^3.3",
        "bower-asset/fuelux": "^3.11",
        "bower-asset/bootswatch": "^3.3",
        "intervention/image": "^2.3",
        "league/commonmark": "^1.0.0",
        "phptal/phptal": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Potherca\\Apache\\Modules\\AutoIndex\\" : "src"
        }
    },
    "extra": {
        "asset-optimize-with-conjunctive" : false
    }
}
